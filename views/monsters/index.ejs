<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("../partials/head.ejs") %>
</head>

<body>
    <%- include("../partials/styles.ejs") %>

        <div class="full-page">
            <header>
                <%- include("../partials/header.ejs") %>
            </header>
            <div class="shell">
                <div class="all-things">
                    <% monsters.forEach(monster=> { %>
                        <li class="indexes"><a href="/monsters/<%=monster._id; %>">
                                <%=monster.name; %>
                            </a></li>
                        <% }) %>
                </div>
            </div>
        </div>

</body>

</html>